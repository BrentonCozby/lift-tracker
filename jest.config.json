{
    "moduleNameMapper": {
        "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js",
        "\\.(css|less)$": "<rootDir>/__mocks__/style-mock.js",
        "images": "<rootDir>/client/static/images/"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
        "**client/js/components/**/*.{js,jsx}",
        "**client/js/actions-and-reducers/**/*.{js,jsx}",
        "**client/js/programs/**/*.{js,jsx}",
        "!**/node_modules/**"
    ],
    "coverageReporters": ["json", "lcov"],
    "globals": {
        "Dir": "",
        "PP": "",
        "SITE_TITLE": "",
        "SITE_NAME": "",
        "DESCRIPTION": "",
        "SITE_URL": "",
        "SITE_IMAGE": "",
        "DEVELOPER_NAME": "",
        "DEVELOPER_URL": "",
        "GOOGLE_ANALYTICS_ID": "",
        "DEV_PATH": "",
        "process.env.NODE_ENV": "",
        "process.env.perishable_key": ""
    },
    "testURL": "https://example.com"
}